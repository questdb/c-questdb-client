digraph line_sender_cpp_api {

  graph [bgcolor=transparent];
  node [
    style=filled,
    shape=box,
    fillcolor="#343942",
    fontcolor="#C9D1D9",
    fontname=monospace];
  edge [color="#343942"];

  line_sender -> table;
  line_sender -> dtor;
  table -> symbol;
  symbol -> symbol;
  symbol -> column;
  symbol -> at;
  table -> column;
  column -> column;
  column -> at;
  at -> flush;
  flush -> close;
  flush -> dtor;
  at -> table;
  flush -> table;
  close -> close;
  close -> dtor;

  line_sender [label="line_sender::line_sender(opts)"];
  table [label=".table(name)"];
  symbol [label=".symbol(name, value)"];
  column [label=".column(name, value)"]
  flush [label=".flush()"];
  close [label=".close()"];
  dtor [label="line_sender::~line_sender()"];
  at [label=".at(timestamp)\n.at_now()"];
}
